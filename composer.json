{
    "name": "bitnoise/errbit-php-bundle",
    "type": "symfony-bundle",
    "description": "errbit-php symfony2 bundle",
    "keywords": ["bitnoise", "errbit", "bug"],
    "homepage": "https://github.com/bitnoise/BtnErrbitBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Bitnoise | Lukasz Roth",
            "homepage": "http://www.bitnoi.se/",
            "email": "labs@bitnoi.se"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.*",
        "emgiezet/errbit-php": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.0.0",
        "fabpot/php-cs-fixer": "~0.5.0",
        "phpmd/phpmd": "~2.0.0",
        "sebastian/phpcpd": "~2.0.0",
        "asm89/twig-lint": "~1.0.1"
    },
    "config": {
        "preferred-install": "source"
    },
    "autoload": {
        "psr-4": { "Btn\\ErrbitBundle\\": "src/Btn/ErrbitBundle/" }
    },
    "extra": {
        "branch-alias": {
            "dev-2.1": "2.1-dev"
        }
    }
}
